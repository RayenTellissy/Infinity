generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Users {
  id         String   @id @default(cuid())
  username   String   @unique
  email      String   @unique
  password   String
  image      String?
  created_at DateTime @default(now())
}
